
@{
    ViewData["Title"] = "Index";
}


<br />
<div class="row">
    <div class="col-6">
        <h2 class="text-primary">Air-Conditioner List </h2>
    </div>
    <div class="col-6 text-right">
        <a asp-controller="AirConditioner" asp-action="Upsert" class="btn btn-primary"><i class="fas fa-plus"></i> &nbsp; Create New Air-Conditioner</a>
    </div>
</div>

<br />
<div class="p-4 border rounded">
    <table id="tblData" class="table table-striped table-bordered" style="width:100%">
        <thead class="thead-dark">
            <tr class="table-info">
                <th>User Name</th>
                <th>Name</th>
                <th>Power Status</th>
                <th>Temperature</th>
                <th>Source Code</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{
    <script src="~/js/airConditioner.js"></script>
}