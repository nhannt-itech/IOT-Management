
@{
    ViewData["Title"] = "Index";
}

<br />
<div class="row">
    <div class="col-6">
        <h2 class="text-primary">Danh sách loại thiết bị </h2>
    </div>
    <div class="col-6 text-right">
        <a class="btn btn-success text-light" id="upsert" asp-area="Admin" asp-controller="DeviceType" asp-action="Upsert">
            <i class="fas fa-plus"></i> &nbsp; Tạo loại thiết bị
        </a>
    </div>
</div>
<br />
<div class="p-4 border rounded">
    <table id="tblData" class="table table-striped table-bordered" style="width:100%">
        <thead class="thead-dark">
            <tr class="table-info">
                <th>Tên</th>
                <th>Ảnh - on</th>
                <th>Ảnh - off</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{
    <script src="~/js/deviceType.js"></script>
}
